{
    "manifestVersion": 1,
    "id": "vsts-publish-adf",
    "version": "2.0.4",
    "name": "Azure Data Factory",
    "publisher": "liprec",
    "public": true,
    "description": "This extension adds release tasks related to Azure Data Factory: publishing JSON, suspend/resume pipelines",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "icons": {
        "default": "images/logo.png",
        "large": "images/logolarge.png"
    },
    "screenshots": [
        { "path": "images/screenshot-1.png" }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "azure",
        "deployment",
        "adf",
        "data factory"
    ],
    "scopes": [
        "vso.release"
    ],
    "branding": {
        "color": "rgb(87, 32, 31)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/liprec/vsts-publish-adf"
        },
        "support": {
            "uri": "https://github.com/liprec/vsts-publish-adf/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/liprec/vsts-publish-adf"
    },
    "contributions": [
        {
            "id": "deploy-adf-json",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "deploy-adf-json"
            }
        },
        {
            "id": "suspend-adf-pipeline",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "suspend-adf-pipeline"
            }
        },
        {
            "id": "toggle-adf-trigger",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "toggle-adf-trigger"
            }
        },
        {
            "id": "delete-adf-items",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "delete-adf-items"
            }
        },
        {
            "id": "trigger-adf-pipeline",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name" : "trigger-adf-pipeline"
            }
        }
    ],
    "files": [
        {
            "path": "deploy-adf-json/1.1.11"
        },
        {
            "path": "suspend-adf-pipeline/1.0.6"
        },
        {
            "path": "toggle-adf-trigger/1.0.4"
        },
        {
            "path": "delete-adf-items/v2/dist"
        },
        {
            "path": "delete-adf-items/v2/node_modules"
        },
        {
            "path": "delete-adf-items/v2/task.json"
        },
        {
            "path": "delete-adf-items/v2/icon.png"
        },
        {
            "path": "deploy-adf-json/v2/dist"
        },
        {
            "path": "deploy-adf-json/v2/node_modules"
        },
        {
            "path": "deploy-adf-json/v2/task.json"
        },
        {
            "path": "deploy-adf-json/v2/icon.png"
        },
        {
            "path": "toggle-adf-trigger/v2/dist"
        },
        {
            "path": "toggle-adf-trigger/v2/node_modules"
        },
        {
            "path": "toggle-adf-trigger/v2/task.json"
        },
        {
            "path": "toggle-adf-trigger/v2/icon.png"
        },
        {
            "path": "trigger-adf-pipeline/v2/dist"
        },
        {
            "path": "trigger-adf-pipeline/v2/node_modules"
        },
        {
            "path": "trigger-adf-pipeline/v2/task.json"
        },
        {
            "path": "trigger-adf-pipeline/v2/icon.png"
        },
        {
            "path": "images/logo.png",
            "addressable": true
        },
        {
            "path": "images/logolarge.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-2.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-3.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-4.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-5.png",
            "addressable": true
        },
        {
            "path": "images/screenshot-6.png",
            "addressable": true
        }
    ]
}